<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>LodStubServlet</servlet-name>
        <servlet-class>my.app.stub.LodStubServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LodJournalServlet</servlet-name>
        <servlet-class>my.app.stub.LodJournalServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>LodStubServlet</servlet-name>
        <url-pattern>/ws/pgu/request/tax/get_lic</url-pattern>
    </servlet-mapping>
    <!--POST http://localhost:8082/ws/pgu/request/tax/get_lic HTTP/1.1-->
    <servlet-mapping>
        <servlet-name>LodJournalServlet</servlet-name>
        <url-pattern>/ws/pgu/journal</url-pattern>
    </servlet-mapping>

</web-app>
